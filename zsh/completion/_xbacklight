arguments=(
  '-display[or -d <display>]'
  '-help[]'
  '-version[]'
  '-set[or = <percentage>]'
  '-inc[or + <percentage>]'
  '-dec[or - <percentage>]'
  '-get[]'
  '-time[time in milliseconds>]'
  '-steps[of steps in fade>]'
  '*:filename:_files'
)

